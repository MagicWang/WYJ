<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:wyj="http://schemas.wyj.com/2014/xaml">
    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type wyj:ResourceKeys}, ResourceId=ButtonNormalBackgroundKey}" Color="Blue"/>
    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type wyj:ResourceKeys}, ResourceId=ButtonMouseOverBackgroundKey}" Color="White"/>
    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type wyj:ResourceKeys}, ResourceId=ButtonPressedBackgroundKey}" Color="#FFB0B0B0"/>
    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type wyj:ResourceKeys}, ResourceId=ButtonNormalForegroundKey}" Color="#CCD6D6D6"/>
    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type wyj:ResourceKeys}, ResourceId=ButtonMouseOverForegroundKey}" Color="#FFC6C6C6"/>
    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type wyj:ResourceKeys}, ResourceId=ButtonPressedForegroundKey}" Color="#FFC6C6C6"/>
</ResourceDictionary>
